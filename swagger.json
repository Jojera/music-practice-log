    "definitions": {
    "MusicLog": {
      "type": "object",
      "required": ["instrument", "duration", "focusArea"],
      "properties": {
        "instrument": {
          "type": "string",
          "description": "The instrument practiced",
          "example": "Drums"
        },
        "duration": {
          "type": "integer",
          "description": "Practice duration in minutes",
          "example": 30
        },
        "focusArea": {
          "type": "string",
          "description": "What the musician focused on during practice",
          "example": "Rudiments"
        },
        "date": {
          "type": "string",
          "example": "any"
        }
      }
    },
    "Musician": {
      "type": "object",
      "required": ["name", "email", "age", "instrument"],
      "properties": {
        "name": {
          "type": "string",
          "description": "Full name of the musician",
          "example": "Andy Harrison"
        },
        "email": {
          "type": "string",
          "format": "email",
          "description": "Email address of the musician",
          "example": "andy@gmail.com"
        },
        "age": {
          "type": "integer",
          "description": "Age of the musician",
          "example": 33
        },
        "instrument": {
          "type": "string",
          "description": "Primary instrument played",
          "example": "Drums"
        },
        "experienceLevel": {
          "type": "string",
          "description": "Skill level of the musician",
          "example": "any"
        }
      }
    }
